#!/bin/sh
# PCP GUI QA Test No. 016
# Test QmcContext functionality
#
seq=`basename $0`
echo "QA output created by $seq"
. ./common.test

status=1	# failure is the default!
trap "rm -f $tmp.*; exit \$status" 0 1 2 3 15

# timezones and EHOSTUNREACH (148 for IRIX, 113 for Linux)
_filter()
{
    sed \
	-e "s#EST-11EST-10,89/2:00,299/2:00#TZ#g" \
	-e 's/hndl = [0-9][0-9]*/hndl = HANDLE/g' \
	-e 's/-113 has 0/-148 has 0/' \
	-e 's/sts = -113/sts = -148/'
}

cd $here/archives
$here/src/qmc_context/qmc_context -DPMC 2>&1 | _filter

# success, all done
status=0
exit
