if [ -d /Library/Receipts/pcp-gui-* ]
then
    /bin/chmod 755 /Library/Receipts/pcp-gui*
    /usr/sbin/chown -R root /Library/Receipts/pcp-gui*
    /usr/bin/chgrp -R wheel /Library/Receipts/pcp-gui*
fi

exit 0
